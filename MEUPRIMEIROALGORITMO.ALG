algoritmo "MeuPrimeiroAlgoritmo"
var
nome:caracter
idade:inteiro
altura:real
casado:logico
inicio
escreva("Digite seu nome: ")
leia(nome)
escreva("Digite sua idade: ")
leia(idade)
escreva("Digite sua altura: ")
leia(altura)
escreva("seu nome é: ",nome, ",sua idade é: ",idade, ",sua altura é: ",altura)
fimalgoritmo